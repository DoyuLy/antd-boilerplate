{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es5",
        "noImplicitAny": false,
        "sourceMap": false,
        "jsx": "preserve",
        "experimentalDecorators": true,
        "skipDefaultLibCheck": true,
        "skipLibCheck": true,
        "allowSyntheticDefaultImports": true
    },
    "awesomeTypescriptLoaderOptions": {
        "useWebpackText": true,
        "useCache": true,
        "useBabel": true
    }
}